		<constraints primaryKey="true" nullable="false"/>
	</column>
	<column name="OrganizationID" type="SERIAL" />
	<column name="EmpID" type="SERIAL" />
</createTable>
  <createTable schemaName="${schemaName}" tableName="EmlpoyeeProfileSkillset">
