	                                 referencedColumnNames="EmpPersonalInfoID" referencedTableSchemaName="${schemaName}"/>
 
	<addColumn tableName="EmlpoyeeProfile" schemaName="${schemaName}">
	    <column name="EmlpoyeeProfileEmployeedetails" type="SERIAL"/>
	</addColumn>
